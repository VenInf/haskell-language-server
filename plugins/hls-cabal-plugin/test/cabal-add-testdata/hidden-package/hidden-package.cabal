cabal-version:       3.4

name:                hidden-package
version:             0.1.0.0
build-type:          Simple

executable hidden-package
  main-is:             Main.hs
  hs-source-dirs:      src
  ghc-options:         -Wall
  build-depends:       base
  default-language:    Haskell2010


library hidden-package
  build-depends:   base >= 4 && < 5
  ghc-options:     -Wall